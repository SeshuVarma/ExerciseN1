<!DOCTYPE html>
<html lang="en">


<body class="text-center">
    <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">

        <h1 class="cover-heading">Class List</h1>
        <table id="Covid Dashboard">
            <tr>
                <th>Name</th>
                <th>Grade</th>
                <th>Year</th>
            </tr>

            <tbody>
                <% covidData.forEach((m)=> { %>
                    <tr>
                        <td>
                            <%= m.name %>
                        </td>
                        <td>
                            <%= m.grade %>
                        </td>
                        <td>
                            <%= m.year %>
                        </td>
                    </tr>
                    <% }); %>
            </tbody>
        </table>

    </div>


</body>

</html>